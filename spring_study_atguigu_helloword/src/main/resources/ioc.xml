<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="com.jiajia.study.bean.Person">
        <property name="lastName" value="玊玊"></property>
        <property name="age" value="22"></property>
        <property name="email" value="susu@qq.com"></property>
        <property name="gender" value="男" ></property>
    </bean>

    <bean id="person3" class="com.jiajia.study.bean.Person">
        <constructor-arg value="佳佳" />
        <constructor-arg value="jiajia@qq.com"></constructor-arg>
        <constructor-arg value="男"></constructor-arg>
        <constructor-arg value="22"></constructor-arg>
    </bean>
</beans>